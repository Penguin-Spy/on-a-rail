{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.penguin_spy",
		"id": "onarail",
		"version": "${version}",
		"metadata": {
			"name": "On A Rail",
			"description": " A Minecraft Quilt mod that fixes Furnace Minecart's train capabilities and makes hoppers work with them too.",
			"contributors": {
				"Penguin_Spy": "Owner"
			},
			"contact": {
				"issues": "https://github.com/Penguin-Spy/onarail/issues",
				"sources": "https://github.com/Penguin-Spy/onarail"
			},
			"icon": "assets/onarail/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "io.github.penguin_spy.onarail.OnARail"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.8-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=5.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.3"
			}
		]
	},
	"mixin": "onarail.mixins.json"
